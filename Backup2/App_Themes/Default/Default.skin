<asp:Button runat="server"
    SkinID="FlatButton" 
    Font-Names="Tahoma" 
    Font-Size="10pt"
    Height="22px" 
    BackColor="#F5F5ED" 
    BorderWidth="1px" 
    BorderColor="#426194"
    BorderStyle="Solid" />

